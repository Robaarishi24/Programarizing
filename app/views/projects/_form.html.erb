<%= form_for @project do |f| %>
  <%= f.hidden_field :programmer_id, :value => @programmer.id %>
  <div>
  	<%= f.label :name, 'Name' %>
  	<%= f.text_field :name %>
	  </div>
	  <div>
  	<%= f.label :description, 'Description' %>
  	<%= f.text_area :description %>
	  </div>
	  <div>
    <%= f.label :link, 'Link' %>
  	<%= f.text_field :link%>
	  </div>
  
  <%= f.submit "Submit" %>
<% end %>