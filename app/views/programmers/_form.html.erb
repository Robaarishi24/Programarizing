<%= form_for @programmer do |f| %>
<div>
  	<%= f.label :first_name, 'First Name'%>
  	<%= f.text_field :first_name%>
  </div>
  <div>
  	<%= f.label :last_name, 'Last Name' %>
  	<%= f.text_field :last_name %>
  </div>
  <div>
  	<%= f.label :phone, 'Phone' %>
  	<%= f.text_field :phone %>
  </div>
  <div>
  	<%= f.label :email, 'Email' %>
  	<%= f.text_field :email%>
  </div>
  <%= f.submit "Submit"%>
<% end %>